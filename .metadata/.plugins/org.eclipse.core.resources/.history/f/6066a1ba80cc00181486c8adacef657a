import javax.swing.*;
public class Entrada_Numeros {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		//double x=10000.0;
		
		//System.out.printf("%1.2f", x/3);
		
		String num1=JOptionPane.showInputDialog("Introduzca un número"); //Se introduce un valor a través de JOptionPane
		
		double num2=Double.parseDouble(num1); //Se pasa a double el string en el que está almacenado el número
		
		System.out.print("La raíz de " + Math.round(num2) + " es "); //Se muestra el número redodeando
		
		System.out.printf("%1.2f", Math.sqrt(num2)); // Se calcula la raiz de ese número
	}

}
