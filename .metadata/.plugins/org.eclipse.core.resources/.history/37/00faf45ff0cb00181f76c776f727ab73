		String alumno1, alumno2;
		
		alumno1="David";
		
		alumno2="David";
		
		boolean Resultado = alumno1.equals(alumno2);
		
		if (Resultado==false) {
			System.out.println("Coincide");}
		else {System.out.println("No coincide");
		}
		//System.out.println(alumno1.equalsIgnoreCase(alumno2));
	}
}